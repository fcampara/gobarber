  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "db:redis": "docker run --name redisbarber -p 6379:6379 -d -t redis:alpine",
    "db:mongo": "docker run --name mongobarber -p 27017:27017 -d -t mongo",
    "db:postgres": "docker run --name database -e POSTGRES_PASSWORD=docker -p 5432:5432 -d postgres",
    "db:run": " npm run db:postgres && npm run db:mongo && npm run db:redis ",
    "db:start": "docker start database && docker start mongobarber && docker start redisbarber",
    "db:stop": "docker stop database && docker stop mongobarber && docker stop redisbarber",
    "db:migrate": "npx sequelize db:migrate",
    "eslint:fix": "./node_modules/.bin/eslint --fix src --ext .js"
  },
